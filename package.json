{
  "name": "kitkat-bot",
  "version": "0.0.0-local.0",
  "sha": "0.0.0-local.0",
  "private": true,
  "license": "MIT",
  "main": "bot/index.ts",
  "homepage": "https://github.com/butter-cookie-kitkat/kitkat-bot",
  "bugs": "https://github.com/butter-cookie-kitkat/kitkat-bot/issues",
  "contributors": [
    "Nick Woodward <nick@salte.io>"
  ],
  "scripts": {
    "build": "ts-node ./scripts/version.ts",
    "start": "nodemon --exec npm run start:dev",
    "start:dev": "ts-node .",
    "start:live": "ts-node -T .",
    "validate": "run-p test:coverage test:integration lint",
    "test": "ts-mocha 'bot/**/__test__/**/*.spec.ts' --timeout 10000 --exit",
    "test:integration": "ts-mocha 'bot/**/__integration__/**/*.spec.ts' --timeout 60000 --exit",
    "test:coverage": "nyc npm run test",
    "test:tdd": "ts-mocha --watch",
    "lint": "eslint --ignore-path .gitignore bot/**/*.ts",
    "lint:fix": "eslint --ignore-path .gitignore --ignore-path .eslintignore --fix"
  },
  "engines": {
    "node": "12.0.x"
  },
  "dependencies": {
    "@butter-cookie-kitkat/discord-core": "git+https://git@github.com/butter-cookie-kitkat/discord-core.git#v1.2.11",
    "canvas": "^2.6.1",
    "cheerio": "^1.0.0-rc.3",
    "conventional-changelog": "^3.1.23",
    "conventional-commits-parser": "^3.1.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.0",
    "outdent": "^0.7.1",
    "pg": "^8.2.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.22.3",
    "sequelize-typescript": "^1.1.0",
    "table": "^5.4.6",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/chance": "^1.1.0",
    "@types/cheerio": "^0.22.21",
    "@types/conventional-commits-parser": "^3.0.0",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.0.3",
    "@types/node-fetch": "^2.5.7",
    "@types/sinon": "^9.0.5",
    "@types/table": "^5.0.0",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.9.0",
    "chai": "^4.2.0",
    "chance": "^1.1.6",
    "eslint": "^7.7.0",
    "eslint-plugin-tsdoc": "^0.2.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "sinon": "^9.0.3",
    "sqlite3": "^5.0.0",
    "ts-mocha": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.json": [
      "git add --force"
    ]
  }
}
