{
  "name": "discord-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "bot/index.js",
  "homepage": "https://github.com/butter-cookie-kitkat/kitkat-bot",
  "bugs": "https://github.com/butter-cookie-kitkat/kitkat-bot/issues",
  "contributors": [
    "Nick Woodward <nick@salte.io>"
  ],
  "scripts": {
    "start": "nodemon --exec npm run start:live",
    "start:live": "node -r esm .",
    "test": "mocha",
    "test:coverage": "nyc mocha",
    "test:tdd": "mocha --watch",
    "lint": "eslint --ignore-path .gitignore bot/**/*.js",
    "lint:fix": "eslint --ignore-path .gitignore --fix"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "ascii-table": "0.0.9",
    "chance": "^1.1.6",
    "cheerio": "^1.0.0-rc.3",
    "conventional-changelog": "^3.1.21",
    "conventional-commits-parser": "^3.1.0",
    "debug": "^4.1.1",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "ffmpeg-static": "^4.2.2",
    "node-fetch": "^2.6.0",
    "outdent": "^0.7.1",
    "pad-left": "^2.1.0",
    "pg": "^8.2.1",
    "sequelize": "^5.21.12",
    "url-parse": "^1.4.7",
    "yargs-parser": "^18.1.3",
    "youtube-node": "^1.3.3",
    "ytdl-core": "^3.1.1",
    "ytdl-core-discord": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jsdoc": "^27.0.5",
    "husky": "^4.2.5",
    "jest-cli": "^26.0.1",
    "lint-staged": "^10.2.9",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "sqlite3": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.json": [
      "git add --force"
    ]
  }
}
