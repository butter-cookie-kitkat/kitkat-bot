{
  "name": "kitkat-bot",
  "version": "0.0.0-local.0",
  "sha": "local",
  "private": true,
  "license": "MIT",
  "main": "bot/index.ts",
  "homepage": "https://github.com/butter-cookie-kitkat/kitkat-bot",
  "bugs": "https://github.com/butter-cookie-kitkat/kitkat-bot/issues",
  "contributors": [
    "Nick Woodward <nick@salte.io>"
  ],
  "scripts": {
    "build": "ts-node ./scripts/version.ts",
    "start": "nodemon --exec npm run start:dev",
    "start:dev": "ts-node .",
    "start:live": "ts-node -T .",
    "validate": "run-p test:coverage test:integration lint",
    "test": "ts-mocha --exit",
    "test:unit": "npm run test -- 'bot/**/__test__/**/*.spec.ts' --timeout 10000",
    "test:integration": "npm run test -- 'bot/**/__integration__/**/*.spec.ts' --timeout 60000",
    "test:coverage": "nyc npm run test:unit",
    "test:tdd": "ts-mocha --watch",
    "lint": "eslint --ignore-path .gitignore bot/**/*.ts",
    "lint:fix": "eslint --ignore-path .gitignore --ignore-path .eslintignore --fix"
  },
  "engines": {
    "node": "12.0.x"
  },
  "dependencies": {
    "@types/chai": "^4.2.11",
    "@types/chance": "^1.1.0",
    "@types/cheerio": "^0.22.18",
    "@types/conventional-commits-parser": "^3.0.0",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^7.0.2",
    "@types/node-fetch": "^2.5.7",
    "@types/sinon": "^9.0.4",
    "@types/table": "^5.0.0",
    "@types/url-parse": "^1.4.3",
    "@butter-cookie-kitkat/discord-core": "git+https://git@github.com/butter-cookie-kitkat/discord-core.git#v1.2.3",
    "@discordjs/opus": "^0.3.2",
    "@ffxiv-teamcraft/crafting-solver": "^1.3.2",
    "@ffxiv-teamcraft/simulator": "^2.5.0",
    "canvas": "^2.6.1",
    "chance": "^1.1.6",
    "cheerio": "^1.0.0-rc.3",
    "conventional-changelog": "^3.1.21",
    "conventional-commits-parser": "^3.1.0",
    "debug": "^4.1.1",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.0",
    "outdent": "^0.7.1",
    "pad-left": "^2.1.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.13",
    "sequelize-typescript": "^1.1.0",
    "table": "^5.4.6",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "url-parse": "^1.4.7",
    "yargs-parser": "^18.1.3",
    "ytdl-core": "^3.1.1",
    "ytdl-core-discord": "^1.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-plugin-tsdoc": "^0.2.5",
    "husky": "^4.2.5",
    "jest-cli": "^26.0.1",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "sqlite3": "^4.2.0",
    "ts-mocha": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.json": [
      "git add --force"
    ]
  }
}
