{
  "name": "discord-bot",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "src/bot.mjs",
  "scripts": {
    "start": "node .",
    "test": "mocha",
    "test:coverage": "nyc mocha",
    "test:tdd": "mocha --watch",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore --fix"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@discordjs/opus": "^0.1.0",
    "dedent": "^0.7.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^4.1.1",
    "is-reachable": "^4.0.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "ytdl-core-discord": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chance": "^1.1.4",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^4.2.5",
    "jest-cli": "^25.4.0",
    "lint-staged": "^10.1.7",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.json": [
      "git add --force"
    ]
  },
  "mocha": {}
}
